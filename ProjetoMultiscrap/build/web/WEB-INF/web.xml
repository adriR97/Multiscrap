<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>SalvaAlbumFoto</servlet-name>
        <servlet-class>Control.SalvaAlbumFoto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>salvaFotoNoAlbum</servlet-name>
        <servlet-class>Control.salvaFotoNoAlbum</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FechaSessao</servlet-name>
        <servlet-class>Control.FechaSessao</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SalvaAlbumFoto</servlet-name>
        <url-pattern>/salvaAlbumFoto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>salvaFotoNoAlbum</servlet-name>
        <url-pattern>/salvaFotoNoAlbum</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FechaSessao</servlet-name>
        <url-pattern>/fechaSessao</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
